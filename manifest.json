{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "webRequest-demo",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.facebook.com",
    "https://www.google.de/*",
    "https://www.googleapis.com/*"
  ],

  "background": {
    "scripts": ["background.js"]
  }

}
